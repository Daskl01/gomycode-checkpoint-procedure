ALGORITHM produit_scalaire

FUNCTION dot_product(ps:integer;v1,v2:array_of_real[1....100]) : return_real
VAR
    i:integer
BEGIN
    read(v1)
    read(v2)
    FOR i FROM 1 TO N STEP 1  DO
        read(v1[i])
    END_FOR
    FOR i FROM 1 TO N STEP 1  DO
        read(v2[i])
    END_FOR

    ps:=0
    FOR i FROM 1 TO N STEP 1  DO
        ps:=ps+v1[i]*v2[i]
    END_FOR
    RETURN ps ;
END